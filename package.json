{
	"name": "zupa",
	"version": "1.0.0",
	"main": "zupa.js",
	"types": "zupa.d.ts",
	"scripts": {
		"start": "npm run start --workspace ./packages/cli",
		"lint": "eslint packages/**/*.ts packages/**/*.js"
	},
	"keywords": [
		"project-management",
		"dependency-manager"
	],
	"author": "Zsolt Lengyel",
	"license": "MIT",
	"bin": {
		"zupa": "zupa.js"
	},
	"engines": {
		"node": ">= 14"
	},
	"files": [
		"*.js",
		"*.d.ts",
		"src/**/*.js"
	],
	"devDependencies": {
		"@babel/eslint-parser": "^7.15.0",
		"@types/node": "^16.6.1",
		"@typescript-eslint/eslint-plugin": "^4.30.0",
		"@typescript-eslint/parser": "^4.30.0",
		"eslint": "^7.32.0",
		"eslint-plugin-node": "^11.1.0"
	},
	"directories": {
		"example": "examples",
		"test": "packages/cli-test"
	},
	"workspaces": [
		"./packages/*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zupajs/zupa.git"
	},
	"bugs": {
		"url": "https://github.com/zupajs/zupa/issues"
	},
	"homepage": "https://github.com/zupajs/zupa#readme",
	"description": "",
	"nyc": {
		"include": [
			"zupa.js",
			"src/**"
		]
	}
}
