{
	"name": "zupa",
	"version": "1.0.0",
	"main": "zupa.js",
	"types": "zupa.d.ts",
	"scripts": {
		"start": "npm run start --workspace ./packages/cli",
		"lint": "eslint zupa.js src/**/* packages/cli-test/**/*"
	},
	"keywords": [
		"project-management",
		"dependency-manager"
	],
	"author": "Zsolt Lengyel",
	"license": "MIT",
	"bin": {
		"zupa": "zupa.js"
	},
	"engines": {
		"node": ">= 14"
	},
	"dependencies": {
		"ink": "^3.0.9",
		"jsonpath-plus": "^6.0.1",
		"rc": "^1.2.8",
		"react": "^16.14.0",
		"strip-ansi": "^7.0.0"
	},
	"files": [
		"*.js",
		"*.d.ts",
		"src/**/*.js"
	],
	"devDependencies": {
		"@babel/eslint-parser": "^7.15.0",
		"@types/node": "^16.6.1",
		"check-engines": "^1.5.0",
		"console-table-printer": "^2.10.0",
		"deepmerge": "^4.2.2",
		"eslint": "^7.32.0",
		"eslint-plugin-node": "^11.1.0"
	},
	"directories": {
		"example": "examples",
		"test": "packages/cli-test"
	},
	"workspaces": [
		"./packages/*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zupajs/zupa.git"
	},
	"bugs": {
		"url": "https://github.com/zupajs/zupa/issues"
	},
	"homepage": "https://github.com/zupajs/zupa#readme",
	"description": "",
	"nyc": {
		"include": [
			"zupa.js",
			"src/**"
		]
	}
}
